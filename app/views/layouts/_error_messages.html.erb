if flash[:notice]
  <div class="alert alert-notice">
    <%= flash[:notice] %>
  </div>
elsif flash[:info]
  <div class="alert alert-info">
    <%= flash[:info] %>
  </div>
elsif flash[:danger]
  <div class="alert alert-danger">
    <%= flash[:danger] %>
  </div>
elsif flash[:warning]
  <div class="alert alert-danger">
    <%= flash[:danger] %>
  </div>
end